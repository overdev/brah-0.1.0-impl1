função add(a, b) {
    retorne a + b;
}

função sub(a, b) {
    retorne a - b;
}

função brah(a, b, c, d) {
    retorne add(a, b) + sub(c, d);
}

função fatorial(x) {
    se (x == 1) {
        retorne 1;
    } senão {
        retorne fatorial(x - 1);
    }
}

função fatorial2(n) {
    retorne n < 2 ? 1 : n * fatorial(n - 1);
}


função principal() {
    escreva brah(70, 30, add(21, 79), 100);
}
