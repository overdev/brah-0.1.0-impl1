
função potência(x: real16, n: int8s): int8s {
    repita (n - 1) {
        x = x * x;
    }
    retorne x;
}


função hipotenusa(x: real16, y: real16): int8s {
    var n: int8s = 2;
    retorne potência(x, n) + potência(y, n);
}


função principal(): int8s {
    var x: real16 = 2.0;
    var y: real16 = 3.0;

    escreva hipotenusa(x, y);

    retorne 0;
}
